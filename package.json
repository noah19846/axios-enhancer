{
  "name": "axios-enhancer",
  "version": "0.0.3",
  "author": "Kisama",
  "license": "MIT",
  "description": "An axios enhancer making it cancelable and retryable",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "lint": "eslint src/**/*.{js,ts} --fix",
    "build": "vite build",
    "gen-types": "node script/build-types.js"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "axios": "^1.2.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jsdom": "^20.0.1",
    "prettier": "^2.7.1",
    "ts-morph": "^16.0.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3"
  },
  "peerDependencies": {},
  "peerDependenciesMeta": {
    "axios": {
      "optional": false
    }
  }
}
